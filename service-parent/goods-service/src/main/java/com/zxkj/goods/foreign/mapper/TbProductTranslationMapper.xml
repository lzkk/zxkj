<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zxkj.goods.foreign.mapper.TbProductTranslationMapper">
    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.zxkj.goods.foreign.entity.TbProductTranslation">
        <id column="id" property="id" jdbcType="BIGINT"/>
        <result column="product_id" property="productId" jdbcType="BIGINT"/>
        <result column="language_code" property="languageCode" jdbcType="CHAR"/>
        <result column="title" property="title" jdbcType="VARCHAR"/>
        <result column="description" property="description" jdbcType="VARCHAR"/>
    </resultMap>
    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, product_id, language_code, title, description
    </sql>
    <insert id="insertAll" parameterType="com.zxkj.goods.foreign.entity.TbProductTranslation" >
        insert into tb_product_translation
        <trim prefix="(" suffix=")" suffixOverrides="," >
            product_id,
            language_code,
            title,
            description,
        </trim>
        VALUES
        <foreach collection="paramList" separator="," item="param" index="index">
            <trim prefix="(" suffix=")" suffixOverrides="," >
                #{param.id, jdbcType=BIGINT},
                #{param.productId, jdbcType=BIGINT},
                #{param.languageCode, jdbcType=CHAR},
                #{param.title, jdbcType=VARCHAR},
                #{param.description, jdbcType=VARCHAR},
            </trim>
        </foreach>
    </insert>
</mapper>